{
  "name": "vext-rag-system",
  "version": "1.0.0",
  "description": "A RAG system built with Vext for document retrieval and question answering",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node --expose-gc --max-old-space-size=4096 src/index.js",
    "dev": "node --expose-gc --max-old-space-size=4096 --watch src/index.js",
    "setup": "node src/setup.js",
    "ingest": "node src/ingest.js",
    "test": "node test/test.js",
    "test:watch": "node --watch test/test.js",
    "test:embedding": "node --expose-gc --max-old-space-size=4096 test_embedding_fix.js"
  },
  "keywords": [
    "rag",
    "vext",
    "retrieval",
    "ai",
    "vector",
    "search",
    "ocr",
    "mistral"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@langchain/openai": "^0.0.14",
    "@mistralai/mistralai": "^1.7.5",
    "@xenova/transformers": "^2.17.2",
    "cheerio": "^1.0.0-rc.12",
    "chromadb": "^1.7.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "groq-sdk": "^0.30.0",
    "jimp": "^0.22.10",
    "langchain": "^0.0.200",
    "mammoth": "^1.6.0",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.20.1",
    "pdf-parse": "^1.1.1",
    "sharp": "^0.33.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
}
